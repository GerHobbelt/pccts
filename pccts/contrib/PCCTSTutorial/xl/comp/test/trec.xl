program testrecord =
   type Q = record 
               q:Integer;
               w:Integer;
            end record;
   type R = record
               x,y:Integer;
               z:Integer;
               r:Q;
            end record;
   type Arr = array [1 .. 3] of R;            
   var A, B : Arr;            
               
begin
   A[1].x := 1;
   A[1].y := 2;
   A[1].z := 3;
   A[1].r.q := 4;
   A[1].r.w := 5;
   A[2].x := 6;
   A[2].y := 7;
   A[2].z := 8;
   A[2].r.q := 9;
   A[2].r.w := 10;  
   A[3].x := 11;
   A[3].y := 12;
   A[3].z := 13;
   A[3].r.q := 14;
   A[3].r.w := 15;   
   
   put(A[1].x);
   put(A[1].y);
   put(A[1].z);
   put(A[1].r.q);
   put(A[1].r.w);
   put(A[2].x);
   put(A[2].y);
   put(A[2].z);
   put(A[2].r.q);
   put(A[2].r.w);
   put(A[3].x) ;
   put(A[3].y );
   put(A[3].z );
   put(A[3].r.q);
   put(A[3].r.w);
   newLine;
   B:=A;
   put(B[1].x);
   put(B[1].y);
   put(B[1].z);
   put(B[1].r.q);
   put(B[1].r.w);
   put(B[2].x);
   put(B[2].y);
   put(B[2].z);
   put(B[2].r.q);
   put(B[2].r.w);
   put(B[3].x) ;
   put(B[3].y );
   put(B[3].z );
   put(B[3].r.q);
   put(B[3].r.w);   
end testrecord.
