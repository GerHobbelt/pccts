// Type errors
// similar to test on p. 277 of Brinch Hansen
program test7 =
   type T = array[1..10] of Integer;
   var x: Integer;
   var y: Boolean;
   var z: T;
   
   procedure p(x : Integer) =
   begin // p
      put(x);
   end p;
   
begin // test7
   y:= not 1 and 2 and 3;
   y:= FALSE*TRUE/FALSE;
   z := z mod z;
   x:= 1 or 2 or 3;
   y := FALSE+TRUE-TRUE;
   z:=z-z;
   if z/= z then
      p(TRUE);
   end if;
end test7.
   
